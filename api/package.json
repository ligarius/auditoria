{
  "name": "auditoria-api",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "esbuild src/server.ts --platform=node --bundle --format=cjs --outfile=dist/server.cjs --sourcemap --packages=external",
    "start": "node dist/server.cjs",
    "prisma": "prisma",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "seed": "node prisma/seed.js",
    "generate": "prisma generate",
    "postinstall": "prisma generate",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.ts"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "archiver": "^6.0.1",
    "bullmq": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "chart.js": "4.4.1",
    "chartjs-node-canvas": "4.1.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.2.0",
    "esbuild": "^0.25.10",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pdfmake": "0.2.10",
    "pdfkit": "^0.13.0",
    "pino-http": "^9.0.0",
    "prom-client": "^15.1.3",
    "puppeteer": "^23.7.1",
    "pino": "^8.15.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.11.19",
    "@types/pdfkit": "^0.13.6",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.1.1",
    "prisma": "5.22.0",
    "tsx": "^4.19.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
