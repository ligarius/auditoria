{
  "name": "auditoria-api",
  "version": "2.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "esbuild prisma/seed.ts --platform=node --bundle --format=cjs --outfile=dist/prisma/seed.cjs && node dist/prisma/seed.cjs",
    "build:bundle": "esbuild src/main.ts --platform=node --bundle --format=cjs --outfile=dist/main.js",
    "build": "tsc -p tsconfig.json && node ./scripts/copy-templates.js && npm run build:bundle",
    "start": "node dist/main.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "postinstall": "npm run generate"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@types/bwip-js": "^3.2.3",
    "archiver": "^6.0.1",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.22.0",
    "bwip-js": "^4.7.0",
    "chart.js": "4.5.0",
    "chartjs-node-canvas": "5.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "esbuild": "^0.25.10",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.4.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.2.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "pdfkit": "^0.13.0",
    "pdfmake": "0.2.10",
    "pino": "^8.15.0",
    "pino-http": "^9.0.0",
    "prisma": "5.22.0",
    "prom-client": "^15.1.3",
    "puppeteer": "^24.10.2",
    "puppeteer-core": "^24.10.2",
    "zod": "^3.22.4",
    "lodash.isequal": "4.5.0"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.11.19",
    "@types/pdfkit": "^0.13.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^15.9.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.1.1",
    "supertest": "^7.1.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.9.2"
  },
  "prisma": {
    "seed": "npm run db:seed"
  }
